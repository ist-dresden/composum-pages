<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Component"
        sling:resourceSuperType="composum/pages/components/composed"
        jcr:description="a rotating set of elements"
        jcr:title="Carousel"
        category="[Teaser,Blueprint]">
    <jcr:content
            jcr:primaryType="nt:unstructured">
        <properties
                jcr:primaryType="nt:unstructured">
            <useControls
                    jcr:primaryType="nt:unstructured"
                    type="Boolean"/>
            <showIndicators
                    jcr:primaryType="nt:unstructured"
                    type="Boolean"/>
            <autoStart
                    jcr:primaryType="nt:unstructured"
                    type="Boolean"/>
            <interval
                    jcr:primaryType="nt:unstructured"/>
            <noPause
                    jcr:primaryType="nt:unstructured"
                    type="Boolean"/>
        </properties>
    </jcr:content>
</jcr:root>
