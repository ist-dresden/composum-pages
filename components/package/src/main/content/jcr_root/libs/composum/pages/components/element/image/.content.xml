<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Component"
        sling:resourceSuperType="composum/pages/components/element/asset"
        jcr:description="a single image element"
        jcr:title="Image"
        category="[Image,General,Blueprint]">
    <jcr:content
            jcr:primaryType="nt:unstructured">
        <properties
                jcr:primaryType="nt:unstructured">
            <imageRef
                    jcr:primaryType="nt:unstructured"/>
            <title
                    jcr:primaryType="nt:unstructured"
                    i18n="{Boolean}true"
                    text="plain"/>
            <alt
                    jcr:primaryType="nt:unstructured"
                    i18n="{Boolean}true"
                    text="plain"/>
            <copyright
                    jcr:primaryType="nt:unstructured"
                    i18n="{Boolean}true"
                    text="plain"/>
            <copyrightUrl
                    jcr:primaryType="nt:unstructured"/>
            <license
                    jcr:primaryType="nt:unstructured"
                    i18n="{Boolean}true"
                    text="plain"/>
            <licenseUrl
                    jcr:primaryType="nt:unstructured"/>
        </properties>
    </jcr:content>
    <i18n
            jcr:primaryType="nt:unstructured">
        <de
                jcr:primaryType="nt:unstructured"
                jcr:description="ein einfaches Bild-Element"
                jcr:title="Bild"/>
    </i18n>
</jcr:root>
