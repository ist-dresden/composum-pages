<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Page">
    <jcr:content
            jcr:primaryType="cpp:PageContent"
            sling:resourceType="composum/pages/components/page/subsite/root"
            sling:target="${site}/home/about/home"
            template="/apps/sites/ist/composum/templates/page/subsite/root">
        <search
                jcr:primaryType="nt:unstructured">
            <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="composum/pages/components/search/field"
                    buttonSymbol="search"
                    searchResultPath="${site}/home/about/meta/search">
            </field>
        </search>
    </jcr:content>
    <meta/>
    <home/>
</jcr:root>
