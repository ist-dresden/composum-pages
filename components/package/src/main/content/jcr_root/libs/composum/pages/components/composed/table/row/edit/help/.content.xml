<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Page">
    <jcr:content
            jcr:primaryType="cpp:PageContent"
            sling:resourceType="composum/pages/stage/edit/tools/component/help/page"
            template="/libs/composum/pages/stage/edit/tools/component/help/template">
        <main
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/stage/edit/tools/component/help/parsys">
            <text_01
                    jcr:primaryType="cpp:Element"
                    sling:resourceType="composum/pages/components/element/text"
                    text="&lt;p>is a container for table cells. The rows can be configured as table head or get a background. After creation of a row, the cells have to be added and configured individually.&amp;nbsp;&lt;/p>">
                <i18n
                        jcr:primaryType="nt:unstructured">
                    <de
                            jcr:primaryType="nt:unstructured"
                            text="Eine Tabellenzeile: ein Container für Tabellenzellen (cell). Die Zeilen können als Tabellenkopf (head) konfiguriert werden, sowie mit Farben hinterlegt werden. Nachdem eine Zeile angelegt wurde, müssen jeweils die Zellen hinzugefügt werden.&lt;br>"/>
                </i18n>
            </text_01>
        </main>
    </jcr:content>
</jcr:root>
